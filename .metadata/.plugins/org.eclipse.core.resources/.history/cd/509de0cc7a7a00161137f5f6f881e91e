package class99;

import java.util.*;

public class reverseVow {
    public String reverse(String input) {
        // Write your solution here.
        HashSet<Character> mySet = new HashSet<Character>();
        mySet.add('a');
        mySet.add('e');
        mySet.add('i');
        mySet.add('o');
        mySet.add('u');
        
        
        
        return input;
    }
    
    public void swap(char[] input, int left, int right) {
        char tmp = input[left];
        input[left] = input[right];
        input[right] = tmp;
    }
}
